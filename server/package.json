  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/socket.io": "^2.1.13",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "socket.io": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "jest": "^26.6.0"
  }
}
